<dataStore>
  <id>${dataStoreId}</id>
  <name>${dataStoreName}</name>
  <enabled>true</enabled>
  <workspace>
    <id>${workspaceId}</id>
  </workspace>
  <connectionParameters>
    <entry key="dbtype">app-schema</entry>
    <entry key="namespace">${uri}</entry>
    <entry key="url">file:./${mappingFileName}</entry>
  </connectionParameters>
  <__default>false</__default>
</dataStore>