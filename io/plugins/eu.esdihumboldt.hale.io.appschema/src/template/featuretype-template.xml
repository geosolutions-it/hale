<featureType>
    <id>${featureTypeId}</id>
    <name>${featureTypeName}</name>
    <nativeName>${featureTypeName}</nativeName>
    <namespace>
        <id>${namespaceId}</id>
    </namespace>
    <title>${featureTypeName}</title>
    <abstract>${featureTypeName}</abstract>
    <srs>EPSG:4326</srs>
    <latLonBoundingBox>
        <minx>-180</minx>
        <maxx>180</maxx>
        <miny>-90</miny>
        <maxy>90</maxy>
        <crs>EPSG:4326</crs>
    </latLonBoundingBox>
    <projectionPolicy>FORCE_DECLARED</projectionPolicy>
    <enabled>true</enabled>
    <metadata>
        <entry key="kml.regionateFeatureLimit">10</entry>
        <entry key="indexingEnabled">false</entry>
        <entry key="cachingEnabled">false</entry>
    </metadata>
    <store class="dataStore">
        <id>${dataStoreId}</id>
    </store>
    <maxFeatures>0</maxFeatures>
    <numDecimals>0</numDecimals>
</featureType>